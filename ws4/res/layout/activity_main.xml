<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg21"
    android:orientation="vertical" >

    <ImageView
        android1:id="@+id/imageView1"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:src="@drawable/ws1" />

    <ImageView
        android1:id="@+id/white"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:src="@drawable/sw" />

    <ImageView
        android1:id="@+id/imageView3"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:src="@drawable/ws" />

    <ImageView
        android1:id="@+id/black"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:src="@drawable/se" />

</LinearLayout>
